pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
            stage('build') {
            
            steps {
                echo 'building'
            }
        }
            stage('deploying') {
            steps {
                echo 'deploying'
                
            }
            }
            stage('test') {
            steps {
                echo 'Testing'
            }
            }
            stage('release') {
            steps {
                echo 'release'
            }
            }
        }
    
}
